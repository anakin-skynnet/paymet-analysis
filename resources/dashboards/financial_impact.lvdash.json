{
  "version": "1.0",
  "name": "Financial Impact Dashboard",
  "displayName": "Financial Impact & ROI Analysis",
  "description": "Transaction value analysis, revenue impact, and financial metrics",
  "tags": [
    "financial",
    "revenue",
    "roi",
    "value"
  ],
  "pages": [
    {
      "name": "financial_impact",
      "displayName": "Financial Impact",
      "layout": [
        {
          "widget": {
            "name": "header",
            "textbox_spec": "## Financial Impact & ROI Analysis\n\nTransaction value metrics, revenue impact, and optimization ROI"
          },
          "position": {
            "x": 0,
            "y": 0,
            "width": 12,
            "height": 2
          }
        },
        {
          "widget": {
            "name": "total_approved_value",
            "queries": [
              {
                "name": "approved_value_kpi",
                "query": {
                  "datasetName": "v_executive_kpis",
                  "query_text": "SELECT approved_value FROM v_executive_kpis LIMIT 1"
                }
              }
            ],
            "spec": {
              "version": 2,
              "widgetType": "counter",
              "encodings": {
                "value": {
                  "fieldName": "approved_value",
                  "displayName": "Approved Value"
                }
              },
              "format": {
                "prefix": "$",
                "notation": "compact"
              }
            }
          },
          "position": {
            "x": 0,
            "y": 2,
            "width": 3,
            "height": 4
          }
        },
        {
          "widget": {
            "name": "total_declined_value",
            "queries": [
              {
                "name": "declined_value_kpi",
                "query": {
                  "datasetName": "v_executive_kpis",
                  "query_text": "SELECT declined_value FROM v_executive_kpis LIMIT 1"
                }
              }
            ],
            "spec": {
              "version": 2,
              "widgetType": "counter",
              "encodings": {
                "value": {
                  "fieldName": "declined_value",
                  "displayName": "Declined Value"
                }
              },
              "format": {
                "prefix": "$",
                "notation": "compact"
              }
            }
          },
          "position": {
            "x": 3,
            "y": 2,
            "width": 3,
            "height": 4
          }
        },
        {
          "widget": {
            "name": "avg_transaction_value",
            "queries": [
              {
                "name": "avg_value_kpi",
                "query": {
                  "datasetName": "v_executive_kpis",
                  "query_text": "SELECT avg_transaction_amount FROM v_executive_kpis LIMIT 1"
                }
              }
            ],
            "spec": {
              "version": 2,
              "widgetType": "counter",
              "encodings": {
                "value": {
                  "fieldName": "avg_transaction_amount",
                  "displayName": "Avg Transaction"
                }
              },
              "format": {
                "prefix": "$"
              }
            }
          },
          "position": {
            "x": 6,
            "y": 2,
            "width": 3,
            "height": 4
          }
        },
        {
          "widget": {
            "name": "total_recovery_potential",
            "queries": [
              {
                "name": "recovery_value_kpi",
                "query": {
                  "datasetName": "v_decline_recovery_opportunities",
                  "query_text": "SELECT SUM(potential_recovery_value) as total_recoverable FROM v_decline_recovery_opportunities WHERE recovery_likelihood IN ('HIGH', 'MEDIUM')"
                }
              }
            ],
            "spec": {
              "version": 2,
              "widgetType": "counter",
              "encodings": {
                "value": {
                  "fieldName": "total_recoverable",
                  "displayName": "Recoverable Value"
                }
              },
              "format": {
                "prefix": "$",
                "notation": "compact"
              }
            }
          },
          "position": {
            "x": 9,
            "y": 2,
            "width": 3,
            "height": 4
          }
        },
        {
          "widget": {
            "name": "value_by_segment",
            "queries": [
              {
                "name": "segment_value_query",
                "query": {
                  "datasetName": "v_merchant_segment_performance",
                  "query_text": "SELECT merchant_segment, total_value, transaction_count, avg_transaction_amount FROM v_merchant_segment_performance ORDER BY total_value DESC"
                }
              }
            ],
            "spec": {
              "version": 2,
              "widgetType": "bar-chart",
              "encodings": {
                "x": {
                  "fieldName": "merchant_segment",
                  "displayName": "Segment"
                },
                "y": [
                  {
                    "fieldName": "total_value",
                    "displayName": "Total Value ($)"
                  }
                ]
              }
            }
          },
          "position": {
            "x": 0,
            "y": 6,
            "width": 6,
            "height": 8
          }
        },
        {
          "widget": {
            "name": "value_by_geography",
            "queries": [
              {
                "name": "geo_value_query",
                "query": {
                  "datasetName": "v_performance_by_geography",
                  "query_text": "SELECT country, total_transaction_value, transaction_count, approval_rate_pct FROM v_performance_by_geography ORDER BY total_transaction_value DESC LIMIT 10"
                }
              }
            ],
            "spec": {
              "version": 2,
              "widgetType": "bar-chart",
              "encodings": {
                "x": {
                  "fieldName": "country",
                  "displayName": "Country"
                },
                "y": [
                  {
                    "fieldName": "total_transaction_value",
                    "displayName": "Total Value ($)"
                  }
                ]
              }
            }
          },
          "position": {
            "x": 6,
            "y": 6,
            "width": 6,
            "height": 8
          }
        },
        {
          "widget": {
            "name": "declined_value_by_reason",
            "queries": [
              {
                "name": "decline_value_query",
                "query": {
                  "datasetName": "v_top_decline_reasons",
                  "query_text": "SELECT decline_reason, total_declined_value, decline_count, avg_amount, recoverable_pct FROM v_top_decline_reasons ORDER BY total_declined_value DESC LIMIT 10"
                }
              }
            ],
            "spec": {
              "version": 2,
              "widgetType": "bar-chart",
              "encodings": {
                "x": {
                  "fieldName": "decline_reason",
                  "displayName": "Decline Reason"
                },
                "y": [
                  {
                    "fieldName": "total_declined_value",
                    "displayName": "Declined Value ($)"
                  }
                ]
              }
            }
          },
          "position": {
            "x": 0,
            "y": 14,
            "width": 12,
            "height": 8
          }
        },
        {
          "widget": {
            "name": "recovery_roi_analysis",
            "queries": [
              {
                "name": "recovery_roi_query",
                "query": {
                  "datasetName": "v_decline_recovery_opportunities",
                  "query_text": "SELECT decline_reason, merchant_segment, decline_count, potential_recovery_value, recovery_likelihood, recommended_action FROM v_decline_recovery_opportunities WHERE recovery_likelihood IN ('HIGH', 'MEDIUM') ORDER BY potential_recovery_value DESC LIMIT 20"
                }
              }
            ],
            "spec": {
              "version": 2,
              "widgetType": "table",
              "encodings": {
                "columns": [
                  {
                    "fieldName": "decline_reason",
                    "displayName": "Decline"
                  },
                  {
                    "fieldName": "merchant_segment",
                    "displayName": "Segment"
                  },
                  {
                    "fieldName": "decline_count",
                    "displayName": "Count"
                  },
                  {
                    "fieldName": "potential_recovery_value",
                    "displayName": "Recovery $"
                  },
                  {
                    "fieldName": "recovery_likelihood",
                    "displayName": "Likelihood"
                  },
                  {
                    "fieldName": "recommended_action",
                    "displayName": "Action"
                  }
                ]
              }
            }
          },
          "position": {
            "x": 0,
            "y": 22,
            "width": 12,
            "height": 8
          }
        },
        {
          "widget": {
            "name": "retry_recovered_value",
            "queries": [
              {
                "name": "retry_value_query",
                "query": {
                  "datasetName": "v_retry_performance",
                  "query_text": "SELECT decline_reason, retry_count, retry_attempts, retry_success_rate_pct, recovered_value, effectiveness FROM v_retry_performance WHERE recovered_value > 0 ORDER BY recovered_value DESC LIMIT 15"
                }
              }
            ],
            "spec": {
              "version": 2,
              "widgetType": "table",
              "encodings": {
                "columns": [
                  {
                    "fieldName": "decline_reason",
                    "displayName": "Decline"
                  },
                  {
                    "fieldName": "retry_count",
                    "displayName": "Retry #"
                  },
                  {
                    "fieldName": "retry_attempts",
                    "displayName": "Attempts"
                  },
                  {
                    "fieldName": "retry_success_rate_pct",
                    "displayName": "Success %"
                  },
                  {
                    "fieldName": "recovered_value",
                    "displayName": "Recovered $"
                  },
                  {
                    "fieldName": "effectiveness",
                    "displayName": "Effectiveness"
                  }
                ]
              }
            }
          },
          "position": {
            "x": 0,
            "y": 30,
            "width": 12,
            "height": 8
          }
        },
        {
          "widget": {
            "name": "value_trend_daily",
            "queries": [
              {
                "name": "value_trend_query",
                "query": {
                  "datasetName": "v_daily_trends",
                  "query_text": "SELECT event_date, total_value, transactions, approval_rate FROM v_daily_trends ORDER BY event_date DESC LIMIT 90"
                }
              }
            ],
            "spec": {
              "version": 2,
              "widgetType": "area-chart",
              "encodings": {
                "x": {
                  "fieldName": "event_date",
                  "displayName": "Date"
                },
                "y": [
                  {
                    "fieldName": "total_value",
                    "displayName": "Total Value ($)"
                  }
                ]
              }
            }
          },
          "position": {
            "x": 0,
            "y": 38,
            "width": 12,
            "height": 8
          }
        }
      ]
    }
  ],
