{
  "version": "1.0",
  "name": "Executive Overview Dashboard",
  "displayName": "Executive Overview - Payment Analysis",
  "description": "High-level KPIs and trends for executives",
  "tags": [
    "executive",
    "kpi",
    "overview"
  ],
  "pages": [
    {
      "name": "executive_summary",
      "displayName": "Executive Summary",
      "layout": [
        {
          "widget": {
            "name": "header",
            "textbox_spec": "## Payment Approval Performance\n\nKey metrics and trends at a glance"
          },
          "position": {
            "x": 0,
            "y": 0,
            "width": 12,
            "height": 2
          }
        },
        {
          "widget": {
            "name": "total_transactions_kpi",
            "queries": [
              {
                "name": "kpi_query",
                "query": {
                  "datasetName": "v_executive_kpis",
                  "query_text": "SELECT total_transactions, approval_rate_pct, total_transaction_value FROM v_executive_kpis LIMIT 1"
                }
              }
            ],
            "spec": {
              "version": 2,
              "widgetType": "counter",
              "encodings": {
                "value": {
                  "fieldName": "total_transactions",
                  "displayName": "Total Transactions"
                }
              }
            }
          },
          "position": {
            "x": 0,
            "y": 2,
            "width": 3,
            "height": 4
          }
        },
        {
          "widget": {
            "name": "approval_rate_kpi",
            "queries": [
              {
                "name": "kpi_query",
                "query": {
                  "datasetName": "v_executive_kpis",
                  "query_text": "SELECT approval_rate_pct FROM v_executive_kpis LIMIT 1"
                }
              }
            ],
            "spec": {
              "version": 2,
              "widgetType": "counter",
              "encodings": {
                "value": {
                  "fieldName": "approval_rate_pct",
                  "displayName": "Approval Rate %"
                }
              },
              "format": {
                "suffix": "%"
              }
            }
          },
          "position": {
            "x": 3,
            "y": 2,
            "width": 3,
            "height": 4
          }
        },
        {
          "widget": {
            "name": "total_value_kpi",
            "queries": [
              {
                "name": "kpi_query",
                "query": {
                  "datasetName": "v_executive_kpis",
                  "query_text": "SELECT total_transaction_value FROM v_executive_kpis LIMIT 1"
                }
              }
            ],
            "spec": {
              "version": 2,
              "widgetType": "counter",
              "encodings": {
                "value": {
                  "fieldName": "total_transaction_value",
                  "displayName": "Total Value"
                }
              },
              "format": {
                "prefix": "$",
                "notation": "compact"
              }
            }
          },
          "position": {
            "x": 6,
            "y": 2,
            "width": 3,
            "height": 4
          }
        },
        {
          "widget": {
            "name": "avg_fraud_score_kpi",
            "queries": [
              {
                "name": "kpi_query",
                "query": {
                  "datasetName": "v_executive_kpis",
                  "query_text": "SELECT avg_fraud_score FROM v_executive_kpis LIMIT 1"
                }
              }
            ],
            "spec": {
              "version": 2,
              "widgetType": "counter",
              "encodings": {
                "value": {
                  "fieldName": "avg_fraud_score",
                  "displayName": "Avg Fraud Score"
                }
              }
            }
          },
          "position": {
            "x": 9,
            "y": 2,
            "width": 3,
            "height": 4
          }
        },
        {
          "widget": {
            "name": "approval_trends",
            "queries": [
              {
                "name": "trend_query",
                "query": {
                  "datasetName": "v_approval_trends_hourly",
                  "query_text": "SELECT * FROM v_approval_trends_hourly ORDER BY hour LIMIT 168"
                }
              }
            ],
            "spec": {
              "version": 2,
              "widgetType": "line-chart",
              "encodings": {
                "x": {
                  "fieldName": "hour",
                  "displayName": "Time"
                },
                "y": [
                  {
                    "fieldName": "approval_rate_pct",
                    "displayName": "Approval Rate %"
                  }
                ]
              }
            }
          },
          "position": {
            "x": 0,
            "y": 6,
            "width": 6,
            "height": 8
          }
        },
        {
          "widget": {
            "name": "geography_performance",
            "queries": [
              {
                "name": "geo_query",
                "query": {
                  "datasetName": "v_performance_by_geography",
                  "query_text": "SELECT * FROM v_performance_by_geography ORDER BY transaction_count DESC LIMIT 15"
                }
              }
            ],
            "spec": {
              "version": 2,
              "widgetType": "bar-chart",
              "encodings": {
                "x": {
                  "fieldName": "country",
                  "displayName": "Country"
                },
                "y": [
                  {
                    "fieldName": "approval_rate_pct",
                    "displayName": "Approval Rate %"
                  }
                ]
              }
            }
          },
          "position": {
            "x": 6,
            "y": 6,
            "width": 6,
            "height": 8
          }
        },
        {
          "widget": {
            "name": "top_decline_reasons",
            "queries": [
              {
                "name": "decline_query",
                "query": {
                  "datasetName": "v_top_decline_reasons",
                  "query_text": "SELECT * FROM v_top_decline_reasons LIMIT 10"
                }
              }
            ],
            "spec": {
              "version": 2,
              "widgetType": "table",
              "encodings": {
                "columns": [
                  {
                    "fieldName": "decline_reason",
                    "displayName": "Decline Reason"
                  },
                  {
                    "fieldName": "decline_count",
                    "displayName": "Count"
                  },
                  {
                    "fieldName": "pct_of_declines",
                    "displayName": "% of Total"
                  },
                  {
                    "fieldName": "total_declined_value",
                    "displayName": "Value"
                  },
                  {
                    "fieldName": "recoverable_pct",
                    "displayName": "Recoverable %"
                  }
                ]
              }
            }
          },
          "position": {
            "x": 0,
            "y": 14,
            "width": 12,
            "height": 8
          }
        }
      ]
    }
  ],
  "datasets": [
    {
      "name": "v_approval_trends_hourly",
      "displayName": "V Approval Trends Hourly",
      "asset_name": "ahs_demos_catalog.ahs_demo_payment_analysis_dev.v_approval_trends_hourly"
    },
    {
      "name": "v_executive_kpis",
      "displayName": "V Executive Kpis",
      "asset_name": "ahs_demos_catalog.ahs_demo_payment_analysis_dev.v_executive_kpis"
    },
    {
      "name": "v_performance_by_geography",
      "displayName": "V Performance By Geography",
      "asset_name": "ahs_demos_catalog.ahs_demo_payment_analysis_dev.v_performance_by_geography"
    },
    {
      "name": "v_top_decline_reasons",
      "displayName": "V Top Decline Reasons",
      "asset_name": "ahs_demos_catalog.ahs_demo_payment_analysis_dev.v_top_decline_reasons"
    }
  ]
}