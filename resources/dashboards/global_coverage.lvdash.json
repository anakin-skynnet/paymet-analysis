{
  "version": "1.0",
  "name": "Global Coverage Dashboard",
  "displayName": "Global Coverage - Payment Analysis by Country",
  "description": "World map and metrics aggregated by country. Transaction volume, approval rates, and value across issuer countries.",
  "tags": [
    "geography",
    "world-map",
    "countries",
    "global",
    "analytics"
  ],
  "pages": [
    {
      "name": "world_map",
      "displayName": "World Map by Country",
      "layout": [
        {
          "widget": {
            "name": "header",
            "textbox_spec": "## Global Coverage\n\nPayment metrics aggregated by country (issuer). Use the map to compare volume and approval rates across regions."
          },
          "position": {
            "x": 0,
            "y": 0,
            "width": 12,
            "height": 2
          }
        },
        {
          "widget": {
            "name": "countries_kpi",
            "queries": [
              {
                "name": "kpi_query",
                "query": {
                  "datasetName": "v_performance_by_geography",
                  "query_text": "SELECT COUNT(DISTINCT country) as unique_countries FROM v_performance_by_geography"
                }
              }
            ],
            "spec": {
              "version": 2,
              "widgetType": "counter",
              "encodings": {
                "value": {
                  "fieldName": "unique_countries",
                  "displayName": "Countries"
                }
              }
            }
          },
          "position": {
            "x": 0,
            "y": 2,
            "width": 3,
            "height": 4
          }
        },
        {
          "widget": {
            "name": "global_volume_kpi",
            "queries": [
              {
                "name": "kpi_query",
                "query": {
                  "datasetName": "v_performance_by_geography",
                  "query_text": "SELECT SUM(transaction_count) as total_transactions FROM v_performance_by_geography"
                }
              }
            ],
            "spec": {
              "version": 2,
              "widgetType": "counter",
              "encodings": {
                "value": {
                  "fieldName": "total_transactions",
                  "displayName": "Total Transactions"
                }
              },
              "format": {
                "notation": "compact"
              }
            }
          },
          "position": {
            "x": 3,
            "y": 2,
            "width": 3,
            "height": 4
          }
        },
        {
          "widget": {
            "name": "global_value_kpi",
            "queries": [
              {
                "name": "kpi_query",
                "query": {
                  "datasetName": "v_performance_by_geography",
                  "query_text": "SELECT SUM(total_transaction_value) as total_value FROM v_performance_by_geography"
                }
              }
            ],
            "spec": {
              "version": 2,
              "widgetType": "counter",
              "encodings": {
                "value": {
                  "fieldName": "total_value",
                  "displayName": "Total Value"
                }
              },
              "format": {
                "prefix": "$",
                "notation": "compact"
              }
            }
          },
          "position": {
            "x": 6,
            "y": 2,
            "width": 3,
            "height": 4
          }
        },
        {
          "widget": {
            "name": "global_approval_kpi",
            "queries": [
              {
                "name": "kpi_query",
                "query": {
                  "datasetName": "v_performance_by_geography",
                  "query_text": "SELECT ROUND(SUM(transaction_count * approval_rate_pct) / NULLIF(SUM(transaction_count), 0), 2) as weighted_approval_rate FROM v_performance_by_geography"
                }
              }
            ],
            "spec": {
              "version": 2,
              "widgetType": "counter",
              "encodings": {
                "value": {
                  "fieldName": "weighted_approval_rate",
                  "displayName": "Approval Rate %"
                }
              },
              "format": {
                "suffix": "%"
              }
            }
          },
          "position": {
            "x": 9,
            "y": 2,
            "width": 3,
            "height": 4
          }
        },
        {
          "widget": {
            "name": "world_map_choropleth",
            "queries": [
              {
                "name": "map_query",
                "query": {
                  "datasetName": "v_performance_by_geography",
                  "query_text": "SELECT country, transaction_count, approval_rate_pct, total_transaction_value, unique_merchants, avg_fraud_score FROM v_performance_by_geography ORDER BY transaction_count DESC"
                }
              }
            ],
            "spec": {
              "version": 2,
              "widgetType": "choropleth",
              "encodings": {
                "key": {
                  "fieldName": "country",
                  "displayName": "Country"
                },
                "value": {
                  "fieldName": "transaction_count",
                  "displayName": "Transactions"
                }
              }
            }
          },
          "position": {
            "x": 0,
            "y": 6,
            "width": 12,
            "height": 12
          }
        },
        {
          "widget": {
            "name": "by_country_table",
            "queries": [
              {
                "name": "table_query",
                "query": {
                  "datasetName": "v_performance_by_geography",
                  "query_text": "SELECT country, transaction_count, approval_rate_pct, total_transaction_value, unique_merchants, avg_fraud_score FROM v_performance_by_geography ORDER BY transaction_count DESC"
                }
              }
            ],
            "spec": {
              "version": 2,
              "widgetType": "table",
              "encodings": {
                "columns": [
                  {
                    "fieldName": "country",
                    "displayName": "Country"
                  },
                  {
                    "fieldName": "transaction_count",
                    "displayName": "Transactions"
                  },
                  {
                    "fieldName": "approval_rate_pct",
                    "displayName": "Approval %"
                  },
                  {
                    "fieldName": "total_transaction_value",
                    "displayName": "Value"
                  },
                  {
                    "fieldName": "unique_merchants",
                    "displayName": "Merchants"
                  },
                  {
                    "fieldName": "avg_fraud_score",
                    "displayName": "Avg Fraud"
                  }
                ]
              }
            }
          },
          "position": {
            "x": 0,
            "y": 18,
            "width": 12,
            "height": 10
          }
        }
      ]
    }
  ],
  "datasets": [
    {
      "name": "v_performance_by_geography",
      "displayName": "V Performance By Geography",
      "asset_name": "ahs_demos_catalog.ahs_demo_payment_analysis_dev.v_performance_by_geography"
    }
  ]
}
