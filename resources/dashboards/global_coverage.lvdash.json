{
  "datasets": [
    {
      "name": "v_performance_by_geography",
      "displayName": "V Performance By Geography",
      "asset_name": "__CATALOG__.__SCHEMA__.v_performance_by_geography"
    }
  ],
  "pages": [
    {
      "name": "world_map",
      "displayName": "World Map by Country",
      "layout": [
        {
          "widget": {
            "name": "header",
            "multilineTextboxSpec": {
              "lines": [
                "## Global Coverage\n",
                "\n",
                "Payment metrics aggregated by country (issuer). Use the map to compare volume and approval rates across regions."
              ]
            }
          },
          "position": {
            "x": 0,
            "y": 0,
            "width": 12,
            "height": 2
          }
        },
        {
          "widget": {
            "name": "countries_kpi",
            "queries": [
              {
                "name": "kpi_query",
                "query": {
                  "datasetName": "v_performance_by_geography",
                  "disaggregated": false
                }
              }
            ],
            "spec": {
              "version": 2,
              "widgetType": "counter",
              "encodings": {
                "value": {
                  "fieldName": "unique_countries",
                  "displayName": "Countries"
                }
              }
            }
          },
          "position": {
            "x": 0,
            "y": 2,
            "width": 3,
            "height": 4
          }
        },
        {
          "widget": {
            "name": "global_volume_kpi",
            "queries": [
              {
                "name": "kpi_query",
                "query": {
                  "datasetName": "v_performance_by_geography",
                  "disaggregated": false
                }
              }
            ],
            "spec": {
              "version": 2,
              "widgetType": "counter",
              "encodings": {
                "value": {
                  "fieldName": "total_transactions",
                  "displayName": "Total Transactions"
                }
              },
              "format": {
                "notation": "compact"
              }
            }
          },
          "position": {
            "x": 3,
            "y": 2,
            "width": 3,
            "height": 4
          }
        },
        {
          "widget": {
            "name": "global_value_kpi",
            "queries": [
              {
                "name": "kpi_query",
                "query": {
                  "datasetName": "v_performance_by_geography",
                  "disaggregated": false
                }
              }
            ],
            "spec": {
              "version": 2,
              "widgetType": "counter",
              "encodings": {
                "value": {
                  "fieldName": "total_value",
                  "displayName": "Total Value"
                }
              },
              "format": {
                "prefix": "$",
                "notation": "compact"
              }
            }
          },
          "position": {
            "x": 6,
            "y": 2,
            "width": 3,
            "height": 4
          }
        },
        {
          "widget": {
            "name": "global_approval_kpi",
            "queries": [
              {
                "name": "kpi_query",
                "query": {
                  "datasetName": "v_performance_by_geography",
                  "disaggregated": false
                }
              }
            ],
            "spec": {
              "version": 2,
              "widgetType": "counter",
              "encodings": {
                "value": {
                  "fieldName": "weighted_approval_rate",
                  "displayName": "Approval Rate %"
                }
              },
              "format": {
                "suffix": "%"
              }
            }
          },
          "position": {
            "x": 9,
            "y": 2,
            "width": 3,
            "height": 4
          }
        },
        {
          "widget": {
            "name": "world_map_choropleth",
            "queries": [
              {
                "name": "map_query",
                "query": {
                  "datasetName": "v_performance_by_geography",
                  "disaggregated": false
                }
              }
            ],
            "spec": {
              "version": 2,
              "widgetType": "choropleth",
              "encodings": {
                "key": {
                  "fieldName": "country",
                  "displayName": "Country"
                },
                "value": {
                  "fieldName": "transaction_count",
                  "displayName": "Transactions"
                }
              }
            }
          },
          "position": {
            "x": 0,
            "y": 6,
            "width": 12,
            "height": 12
          }
        },
        {
          "widget": {
            "name": "by_country_table",
            "queries": [
              {
                "name": "table_query",
                "query": {
                  "datasetName": "v_performance_by_geography",
                  "disaggregated": false
                }
              }
            ],
            "spec": {
              "version": 2,
              "widgetType": "table",
              "encodings": {
                "columns": [
                  {
                    "fieldName": "country",
                    "displayName": "Country"
                  },
                  {
                    "fieldName": "transaction_count",
                    "displayName": "Transactions"
                  },
                  {
                    "fieldName": "approval_rate_pct",
                    "displayName": "Approval %"
                  },
                  {
                    "fieldName": "total_transaction_value",
                    "displayName": "Value"
                  },
                  {
                    "fieldName": "unique_merchants",
                    "displayName": "Merchants"
                  },
                  {
                    "fieldName": "avg_fraud_score",
                    "displayName": "Avg Fraud"
                  }
                ]
              }
            }
          },
          "position": {
            "x": 0,
            "y": 18,
            "width": 12,
            "height": 10
          }
        }
      ],
      "pageType": "PAGE_TYPE_CANVAS"
    }
  ]
}
